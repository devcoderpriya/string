#include<stdio.h>
#include<string.h>
int main(){
    char ch[100];
    printf("enter the string:");
    scanf("%s",ch);
    char rev[100];
    strcpy(rev,ch);
    strrev(rev);
    if(strcpy(rev,ch)==0){
        printf("palindrome");
    }
     else{
        printf("not palindrome");
     }
    return 0;
}
