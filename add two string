#include<stdio.h>
#include<string.h>
int main(){
    char str1[100];
    printf("enter the streing 1st:");
    scanf(" %s",str1);

    char str2[100];
    printf("enter the streing 1st:");
    scanf(" %s",str2);

    int i=0,j=0;
    while(str1[i] != '\0'){
        i++;
    }
    while(str2[j] != '\0'){
        str1[i]=str2[j];
        i++;
        j++;
    }
    str1[i]='\0';
    printf("new string %s",str1);
    return 0;
}
